{
  "name": "ethstatedb",
  "description": "Command to check Ethereum state database",
  "version": "0.0.3",
  "author": "Sheng Hu <shenghubj@gmail.com>",
  "bin": {
    "ethstatedb": "./bin/run"
  },
  "bugs": "https://github.com/shenghu/ethstatedb/issues",
  "dependencies": {
    "@oclif/command": "^1.5.13",
    "@oclif/config": "^1.12.12",
    "@oclif/plugin-help": "^2.1.6",
    "@types/async": "^2.4.1",
    "@types/bn.js": "^4.11.5",
    "@types/fs-extra": "^7.0.0",
    "cli-ux": "^5.2.1",
    "ethereumjs-account": "^3.0.0",
    "ethereumjs-block": "^2.2.0",
    "ethereumjs-blockchain": "^3.4.0",
    "ethereumjs-util": "^6.1.0",
    "ethereumjs-vm": "^3.0.0",
    "ethjs-util": "^0.1.6",
    "fs-extra": "^8.0.1",
    "level": "^5.0.1",
    "leveldown": "^5.0.2",
    "levelup": "^4.0.1",
    "merkle-patricia-tree": "^3.0.0",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@oclif/dev-cli": "^1.22.0",
    "@oclif/test": "^1.2.4",
    "@oclif/tslint": "^3.1.1",
    "@semantic-release/commit-analyzer": "^6.1.0",
    "@semantic-release/git": "^7.0.8",
    "@semantic-release/release-notes-generator": "^7.1.4",
    "@types/chai": "^4.1.7",
    "@types/leveldown": "^4.0.0",
    "@types/levelup": "^3.1.0",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.14.5",
    "chai": "^4.2.0",
    "coveralls": "^3.0.3",
    "globby": "^8.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "prettier": "^1.17.1",
    "semantic-release": "^15.13.12",
    "source-map-support": "^0.5.12",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/shenghu/ethstatedb",
  "keywords": [
    "ethereum",
    "blockchain"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "ethstatedb",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "check-coverage": true,
    "per-file": false,
    "lines": 90,
    "statements": 90,
    "functions": 90,
    "branches": 50,
    "reporter": [
      "lcov",
      "text",
      "text-summary",
      "cobertura"
    ],
    "report-dir": ".coverage",
    "exclude": [
      "test/**"
    ]
  },
  "release": {
    "branch": "master",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "package-lock.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "repository": "shenghu/ethstatedb",
  "scripts": {
    "coveralls": "coveralls <.coverage/lcov.info",
    "lint": "tslint -p test -t stylish",
    "format": "prettier --config .prettierrc --write \"{src,tests}/**/*.{ts,js}\"",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "npm run format && npm run lint",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\""
  },
  "types": "lib/index.d.ts"
}
